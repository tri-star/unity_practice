git_url("https://github.com/tri-star/apple-certs.git")

storage_mode("git")

type("adhoc") # The default type, can be: appstore, adhoc, enterprise or development

app_identifier([ENV["APPLE_APP_ID"]])
username(ENV["FASTLANE_USERNAME"])

